<?xml version="1.0" encoding="UTF-8"?>
<mts xmlns="https://maiamy.cn/schema/swift"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:schemaLocation="https://maiamy.cn/schema/swift 
                         https://maiamy.cn/schema/swift/swift-mts.xsd">
    
	<mt798 id="mt719Config" index-message-type="719" sub-message-type="719" corporate-to-bank="true">
        <catalogue>Common Group Messages</catalogue>
        <transaction>Response to a DraftUndertaking</transaction>
        <description>Response to a Draft Undertaking Index</description>
        <fields>
            <field no="2.1" tag="27A" field-name="Message Index/Total" format="1!n/1!n" status="M" />
            <field no="2.2" tag="21A" field-name="Customer Reference Number" format="16x" status="M" />
            <field no="2.3" tag="21T" field-name="Customer Business Reference" format="16x" status="M" />
            <field no="2.4" tag="21P" field-name="Bank Reference Number" format="16x" status="M" />
            <field no="2.5" tag="21S" field-name="Bank Business Reference" format="16x" status="M" />
            <field no="2.6" tag="13E" field-name="Message Creation Date Time" format="8!n4!n" pattern="YYYYMMDDHHMM" status="M" />
            <field no="2.7" tag="12K" field-name="Draft Text Version" format="2!n" status="M" />
            <field no="2.8" tag="25G" field-name="Approval for Issuance" format="4!c" status="O" options="ACCP" />
            <field no="2.9" tag="77J" field-name="Comments on Draft Undertaking" format="70*50z" status="O" />
            <field no="2.10" tag="72Z" field-name="Corporate to Bank Information" format="6*35z" status="O" />
            <field no="2.11" tag="23X" field-name="File Identification" format="4!c/65x" status="O">
            	<components>
            		<component index="1" label="Code" format="4!c" status="M" options="FACT|FAXT|EMAL|MAIL|COUR|HOST|OTHR"/>
            		<component index="2" label="File Name or Reference" format="65x" status="M"/>
            	</components>
            </field>
            <field no="2.12" tag="29S" field-name="Customer Identifier" format="4!c/35x" status="O">
            	<components>
            		<component index="1" label="Code" format="4!c" status="M" options="BICC|OTHR"/>
            		<component index="2" label="Party Identifier" format="35x" status="M"/>
            	</components>
            </field>
            <field no="2.13" tag="29P" field-name="Processing Bank Identifier" format="4!a2!a2!c[3!c]" status="O" />
            <field no="2.14" tag="29U" field-name="Lead Bank Identifier" format="4!a2!a2!c[3!c]" status="O" />
        </fields>
        <rules>
            <rule expression-string="#block.getTagByName('25G') == null and #block.getTagByName('77J') == null" 
                  error-message="25G or 77J must be present"/>
        </rules>
    </mt798>
    
</mts>
